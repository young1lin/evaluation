<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.luckyray.evaluation.mapper.AccessRecordMapper">

    <resultMap id="user" type="cn.luckyray.evaluation.entity.AccessRecord">
        <result column="access_ip_Address" property="accessIpAddress"/>
    </resultMap>

    <select id="selectCountAccessIp" resultType="int" parameterType="long">
        SELECT
          COUNT(*)
        FROM
          access_record
        WHERE
          access_ip_address=#{ip}
    </select>

</mapper>